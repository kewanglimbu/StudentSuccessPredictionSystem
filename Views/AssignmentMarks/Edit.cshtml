@model StudentSuccessPrediction.Models.AssignmentMarkViewModel

<h2>Edit</h2>

<form asp-action="Edit">
    <input type="hidden" asp-for="Id" />

    <div class="form-group">
        <label asp-for="SubjectId" class="control-label"></label>
        <select asp-for="SubjectId" asp-items="@Model.Subjects"></select>
    </div>
    <div class="form-group">
        <label asp-for="AssignmentMarkobtained" class="control-label"></label>
        <input asp-for="AssignmentMarkobtained" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
        <a asp-action="Index" class="btn btn-secondary">Cancel</a>
    </div>
</form>
